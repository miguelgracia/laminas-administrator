{
    "name": "laminas/skeleton-application",
    "description": "Skeleton Application for Zend Framework zend-mvc applications",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "mvc",
        "zf"
    ],
    "homepage": "http://framework.zend.com/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "laminas/laminas-mvc": "^3.1",
        "laminas/laminas-development-mode": "^3.2",
        "laminas/laminas-cache": "^2.9",
        "laminas/laminas-db": "^2.11",
        "laminas/laminas-json": "^3.1",
        "laminas/laminas-log": "^2.12",
        "laminas/laminas-mvc-console": "^1.2",
        "laminas/laminas-mvc-i18n": "^1.1",
        "laminas/laminas-mvc-plugins": "^1.0",
        "laminas/laminas-psr7bridge": "^1.2",
        "laminas/laminas-session": "^2.9",
        "laminas/laminas-servicemanager-di": "^1.2",
        "evandotpro/edp-module-layouts": "^1.0",
        "intervention/image": "^2.5",
        "cocur/slugify": "^4.0",
        "laminas/laminas-i18n": "^2.10",
        "laminas/laminas-code": "^3.4",
        "laminas/laminas-paginator": "^2.8",
        "laminas/laminas-mail": "^2.12",
        "laminas/laminas-navigation": "^2.9",
        "laminas/laminas-i18n-resources": "^2.6",
        "laminas/laminas-dependency-plugin": "^1.0",
        "laminas/laminas-mime": "^2.7",
        "laminas/laminas-form": "^2.15"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/",
            "Autoload\\": "autoload/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/"
        }
    },
    "extra": [],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "development-disable": "laminas-development-mode disable",
        "development-enable": "laminas-development-mode enable",
        "development-status": "laminas-development-mode status",
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "serve": "php -S 0.0.0.0:8080 -t public public/index.php",
        "test": "phpunit"
    },
    "require-dev": {
        "laminas/laminas-developer-tools": "^1.1.0",
        "laminas/laminas-test": "^3.0.1"
    }
}
